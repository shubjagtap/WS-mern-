
function majority(arr) {
    let n = arr.length;  
    for (let i = 0; i < n; i++) {
        let count = 0;

     for (let j = 0; j < n; j++) {
            if (arr[i] === arr[j]) {
                count++;
            }}
        if (count > n / 2) {
            return arr[i];
        }}
    return -1;}
let arr = [3, 3, 4, 3, 2, 3, 3];
console.log(majority(arr));

